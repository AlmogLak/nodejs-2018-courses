{
    "manifest_version": 2,
    "name": "Group Shopping",
    "description": "Group shopping in Amazon, Ebay and Ali express...",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    }
}